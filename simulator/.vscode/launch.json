{
  "version": "0.2.0",
  "configurations": [{
    "program": "${workspaceRoot}/output/main",
    "name": "Debug",
    "type": "cppdbg",
    "request": "launch",
    "args": ["tests/store_and_use.in", "10", "2", "1"],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}",
    "environment": [],
    "externalConsole": false,
    "linux": {
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "program": "${workspaceFolder}/output/main"
    },
    "osx": {
      "MIMode": "lldb",
      "miDebuggerPath": "lldb-mi",
      "program": "${workspaceFolder}/output/main"
    },
    "windows": {
      "MIMode": "gdb",
      "miDebuggerPath": "gdb.exe",
      "program": "${workspaceFolder}/output/main.exe"
    },
    "preLaunchTask": "build"
  }]
}